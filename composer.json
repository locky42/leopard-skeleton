{
    "name": "locky42/leopard-skeleton",
    "description": "A skeleton application for Leopard PHP Framework",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "Максим Зінченко",
            "email": "zinchenko.m.42@gmail.com"
        }
    ],
    "version": "1.1.1",
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Tests\\": "tests/"
        },
        "files": [
            "bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Leopard\\Core\\Tests\\": "vendor/locky42/leopard-core/tests/",
            "Leopard\\Events\\Tests\\": "vendor/locky42/leopard-events/tests/"
        }
    },
    "require": {
        "php": "^8.3",
        "erusev/parsedown": "^1.7",
        "locky42/leopard-core": "^1.1.1",
        "symfony/console": "^7.3",
        "symfony/cache": "^7",
        "guzzlehttp/guzzle": "^7.10",
        "doctrine/orm": "^3",
        "doctrine/dbal": "^4",
        "locky42/leopard-doctrine": "^1.0",
        "locky42/leopard-events": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "scripts": {
        "test": "phpunit --testsuite default,leopard-core,leopard-events",
        "test:default": "phpunit --testsuite default",
        "test:core": "phpunit --testsuite leopard-core",
        "test:events": "phpunit --testsuite leopard-events"
    }
}
