{
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Leopard\\Core\\Tests\\": "vendor/locky42/leopard-core/tests/"
        }
    },
    "repositories": [
        {
          "type": "vcs",
          "url": "https://github.com/locky42/leopard-core.git"
        }
    ],
    "require": {
        "php": "^8.3",
        "erusev/parsedown": "^1.7",
        "locky42/leopard-core": "dev-main",
        "symfony/console": "^7.3",
        "symfony/cache": "^7",
        "guzzlehttp/guzzle": "^7.10",
        "doctrine/orm": "^3",
        "doctrine/dbal": "^4"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    }
}
